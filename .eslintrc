{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
//        "eslint:recommended",
//        "plugin:react/recommended",
        "prettier",
        "airbnb"
    ],
    "plugins": [
        "import",
        "react-hooks"
    ],
    "rules": {
        "indent": 0,
        "linebreak-style": 0,
        "no-tabs": 0,
        "quotes": "off"
    }
}