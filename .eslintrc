{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
//        "airbnb",
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"

    ],
    "plugins": [
        "import",
        "react-hooks"
    ],
    "rules": {
        "indent": 0,
        "linebreak-style": 0,
        "no-tabs": 0,
        "quotes": "off",
        "object-curly-newline": "off",
        "require-yield": "off",
        "no-unused-vars": "off",
        "spaced-comment": "off",
        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-one-expression-per-line": 0,
        "react/jsx-indent": 0,
        "react/no-array-index-key": 0,
        "no-underscore-dangle": "off",
        "react/forbid-prop-types": "off"
    }
}